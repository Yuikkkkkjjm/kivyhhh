<WidgetsLayoutExample@GridLayout>:
    cols:3
    ToggleButton:
        text:"off"
        size_hint:None,1
        width:100
        on_state:root.on_toggle_button_state(self)
    Button:
        text:"count"
        on_press:root.on_Button_Click()
        disabled:not root.s
    Label:
        
        text:root.my_text
        font_size: 40
	    font_name: "comic"
           
    Switch:
        id:swi
        on_active:root.on_switch_active(self)
        size_hint:None,1
        width:100 
    Slider:
        id:my
       # on_value:root.on_slider_value(self)
        min:0
        max:100
        value:50
        disabled:not swi.active
    BoxLayout:
        orientation: "vertical"   
        Label:
            text:str(int(my.value)) #root.my
            font_size: 40
	        font_name   : "comic"
        ProgressBar:
            max:100
            value:my.value

